{
    "manifest_version": 2,
    "name": "Budge",
    "version": "0.0.1",
    "description": "An evolving budgeting utility",
    "icons": {
        "16": "icons/icon_16.png",
        "24": "icons/icon_24.png",
        "32": "icons/icon_32.png",
        "128": "icons/icon_128.png"
    },
    "browser_action": {
        "default_icon": "icons/icon_16.png",
        "default_popup": "popup.html"
    },
    "permissions": ["storage"]
}
